@model List<MyTasks.Domain.DataContracts.TicketListItem>

<div class="CSSTableGenerator">

    <table>
        <tr>
            <td>ProjectName</td>
           
        </tr>

        @for (int i = 0; i < Model.Count(); i++)
        {
            <tr>
                <td>
                    @Model[i].ProjectName
                </td>
            </tr>
        }
    </table>
</div>
